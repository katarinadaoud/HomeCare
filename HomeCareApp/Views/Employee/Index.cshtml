@{
    ViewData["Title"] = "Employee schedule";
    Layout = "_Layout";
}

<main id="main" class="container app-container" tabindex="-1">
  <section aria-labelledby="kal-heading" class="mt-4 pt-4">
    <header class="mb-2">
      <h1 id="kal-heading" class="h4 fw-bold mb-0">Employee schedule</h1>
